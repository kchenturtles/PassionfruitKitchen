{
    "name": "passionfruitkitchen",
    "version": "2.1.0",
    "description": "Passionfruit Kitchen is a website for sharing recipes.",
    "main": "index.js",
    "repository": "https://github.com/kchenturtles/PassionfruitKitchen.git",
    "author": "kchenturtles",
    "license": "MIT",
    "scripts": {
        "start": "hugo server --disableFastRender",
        "start-examplesite": "hugo server -s exampleSite --themesDir=../.. --disableFastRender",
        "build-examplesite": "NODE_ENV=production hugo -s exampleSite --themesDir=../.. --gc",
        "build": "NODE_ENV=production hugo --gc",
        "build:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch"

    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.4.0",
        "autoprefixer": "^10.2.5",
        "postcss": "^8.2.10",
        "postcss-cli": "^8.3.1",
        "postcss-import": "^14.0.1",
        "tailwindcss": "^2.2.19"
    },
    "browserslist": [
        "last 1 version",
        "> 1%",
        "maintained node versions",
        "not dead"
    ],
    "dependencies": {
        "@mailerlite/mailerlite-nodejs": "^1.1.0",
        "mailerlite": "^0.4.0",
        "wrangler": "^3.2.0"
    }
}
